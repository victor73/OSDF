{
    "env": {
        "browser": false,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console":0,
        "no-unused-vars":0,
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            2, "single", "avoid-escape"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
